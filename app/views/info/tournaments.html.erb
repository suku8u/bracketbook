<h1>All Tournaments</h1>

<table class="table table-striped">
  <tr>
    <th>Bracket</th>
    <th>User</th>
  </tr>

<% @brackets.each do |bracket| %>
  <tr>
    <td><%= link_to bracket.name, bracket %></td>
    <td><%= link_to bracket.user.username, profile_path(bracket.user.username) %></td>
  </tr>
<% end %>
</table>

<%= will_paginate(@brackets) %>
